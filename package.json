{
  "name": "runtime-manager",
  "version": "0.1.0",
  "description": "运行管理中心前端",
  "scripts": {
    "hui": "npm i hui hui-theme-cli",
    "posthui": "npm run theme",
    "theme": "node build/theme.js && hui-theme",
    "icons": "node build/icons.js",
    "dev": "cross-env NODE_ENV=development webpack-dev-server --open --hot",
    "prebuild": "rimraf dist/ && npm run icons && npm run hui && npm test",
    "build": "cross-env NODE_ENV=production webpack --hide-modules",
    "lint:css": "stylelint src/**/*.vue",
    "lint:js": "eslint src test --ext=js,vue",
    "lint": "npm run lint:js",
    "preunit": "npm run theme && npm run icons",
    "unit": "karma start",
    "test": "npm run lint && npm run unit"
  },
  "repository": {
    "type": "git",
    "url": "http://10.33.43.73/runtime-manager/runtime-manager"
  },
  "author": "",
  "license": "UNLICENSED",
  "private": true,
  "browserslist": [
    "defaults",
    "ie >= 9"
  ],
  "hui-theme": {
    "browsers": [
      "defaults",
      "ie >= 9"
    ],
    "out": "./temp/theme",
    "config": "./temp/theme.css",
    "theme": "hui/packages/theme-default",
    "minimize": false
  },
  "dependencies": {
    "axios": "^0.18.0",
    "ckeditor": "4.8.0",
    "clipboard": "^2.0.1",
    "d3-force": "^1.1.0",
    "date-fns": "^2.0.0-alpha.8",
    "echarts": "4.0.2",
    "hui": "git+ssh://git@git.hikvision.com.cn:7999/hik/hui.git#v1.2.3",
    "js-cookie": "^2.2.0",
    "lodash": "^4.17.10",
    "normalize.css": "^7.0.0",
    "sha.js": "^2.4.11",
    "vue": "^2.5.16",
    "vue-i18n": "^7.7.0",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.1",
    "vuex-router-sync": "^5.0.0"
  },
  "devDependencies": {
    "@mapbox/stylelint-processor-arbitrary-tags": "^0.2.0",
    "@vue/test-utils": "1.0.0-beta.10",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.3",
    "babel-loader": "^7.1.2",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "chai": "^4.1.2",
    "cross-env": "^5.1.5",
    "css-loader": "^0.28.11",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-vue": "^4.5.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.6",
    "fs-extra": "^6.0.1",
    "html-webpack-plugin": "^3.2.0",
    "hui-theme-cli": "git+http://10.33.43.73/hui/hui-theme-cli.git",
    "istanbul-instrumenter-loader": "^3.0.1",
    "karma": "^2.0.2",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage-istanbul-reporter": "^2.0.0",
    "karma-mocha": "^1.3.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "0.0.32",
    "karma-webpack": "^3.0.0",
    "mocha": "^5.2.0",
