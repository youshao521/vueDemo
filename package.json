    "raw-loader": "^0.5.1",
    "rimraf": "^2.6.2",
    "stylelint": "^9.2.1",
    "stylelint-config-standard": "^18.2.0",
    "swagger-express-middleware": "^1.0.0-alpha.12",
    "url-loader": "^1.0.1",
    "vue-loader": "^14.2.2",
    "vue-template-compiler": "^2.5.16",
    "webfonts-generator": "^0.4.0",
    "webpack": "^3.12.0",
    "webpack-dev-server": "^2.11.2"
  }
